{
  "name": "@mentoc/utils",
  "version": "1.0.9",
  "description": "A library of useful functions.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha tests/src/*/*.js",
    "pluck": "node ./pluck.js",
    "pluck-except": "node ./pluck.js --except",
    "pluck-except-minified": "node ./pluck.js --except --minified",
    "pluck-minified": "node ./pluck.js --minified",
    "pluck-minified-except": "node ./pluck.js --minified --except",
    "minify-export": "node ./node_modules/babel-minify/bin/minify.js ./dist/lib.js -o ./dist/lib.min.js",
    "export": "node ./pluck.js --all",
    "export-minified": "node ./pluck.js --all --minified"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmerfalen/js-utils.git"
  },
  "keywords": [
    "utils",
    "utility",
    "functions"
  ],
  "author": "William Merfalen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wmerfalen/js-utils/issues"
  },
  "homepage": "https://github.com/wmerfalen/js-utils#readme",
  "devDependencies": {
    "babel-minify": "^0.5.1",
    "mocha": "^9.1.1"
  }
}
